package com.myspring.step01;

public class AppleSpeaker implements Speaker {
	Speaker speaker;

	public AppleSpeaker (Speaker speaker) {
		this.speaker = speaker;
		System.out.println("局敲 胶乔目 积己");
	}
	
	@Override
	public void volumnUp () {
		System.out.println("局敲 胶乔目: 家府 农霸");
	}
	
	@Override
	public void volumnDown () {
		System.out.println("局敲 胶乔目: 家府 累霸");
	}
}